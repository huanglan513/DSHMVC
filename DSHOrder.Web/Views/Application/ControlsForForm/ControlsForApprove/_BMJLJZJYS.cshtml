@model DSHOrder.Web.Models.GroupByFlowInfo
@using DSHOrder.Web.Common.Application.GroupByGroup;
@{
    GBGField fh = ViewData["fh"] as GBGField;
}
<table id="GeneralTable">
    <tr>
        <td colspan="2" class="title">
           <b>部门经理及总监一审</b>
        </td>
    </tr>
    <tr>
        <td class="title" width="180">
            备注
        </td>
        <td style="height: 100px" align="left" valign="top">
            @fh.ShowField(m => m.GroupByGroup.ApproveBMJLMemo, GBGFlowEnumEditType.TextArea)
            @Html.ValidationMessageFor(m => m.GroupByGroup.ApproveBMJLMemo)
        </td>
    </tr>
    <tr>
        <td class="title">
            审批时间
        </td>
        <td>
            @fh.ShowField(m => m.GroupByGroup.ApproveBMJLActionTime)
        </td>
    </tr>
</table>
