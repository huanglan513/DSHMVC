@model IEnumerable<DSHOrder.Entity.Function>

@{
    ViewBag.Title = "Function";
}

<h2>Index</h2>
<table width="90%">
 <tr style="text-align:left">
        <th>
            图书名称
        </th>
        <th>
            作者
        </th>
        
        <th>操作</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@Html.DisplayFor(modelItem => item.FunctionID)</td>
            <td>@Html.DisplayFor(modelItem => item.FunctionName)</td>
            <td>
                @Html.ActionLink("编辑", "Edit", new { id = item.FunctionID })|
                @Html.ActionLink("查看", "Details", new { id = item.FunctionID })|
                @Html.ActionLink("删除", "Delete", new { id = item.FunctionID })
            </td>
        </tr>
    }
    </table>

